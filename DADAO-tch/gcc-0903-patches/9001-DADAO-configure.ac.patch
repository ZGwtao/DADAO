From 6c49d3901726324a8c84881b2ff891efc0298519 Mon Sep 17 00:00:00 2001
From: Guan Xuetao <gxt@pku.edu.cn>
Date: Wed, 8 Apr 2020 11:44:36 +0000
Subject: [PATCH] DADAO: configure.ac

Signed-off-by: Guan Xuetao <gxt@pku.edu.cn>
---
 configure.ac | 10 ++++++++++
 1 file changed, 10 insertions(+)

diff --git a/configure.ac b/configure.ac
index 9db4fd14aa2..9cbbe1bf404 100644
--- a/configure.ac
+++ b/configure.ac
@@ -711,6 +711,10 @@ case "${target}" in
     # See <http://gcc.gnu.org/ml/gcc-patches/2004-11/msg00572.html>.
     unsupported_languages="$unsupported_languages fortran"
     ;;
+  dadao-*-*)
+    # See <http://gcc.gnu.org/ml/gcc-patches/2004-11/msg00572.html>.
+    unsupported_languages="$unsupported_languages fortran"
+    ;;
 esac
 
 # Disable libffi for some systems.
@@ -765,6 +769,9 @@ case "${target}" in
 	noconfigdirs="$noconfigdirs target-libffi";;
     esac
     ;;
+  dadao-*-*)
+    noconfigdirs="$noconfigdirs target-libffi"
+    ;;
   hppa*64*-*-hpux*)
     noconfigdirs="$noconfigdirs target-libffi"
     ;;
@@ -1029,6 +1036,9 @@ case "${target}" in
   d30v-*-*)
     noconfigdirs="$noconfigdirs gdb"
     ;;
+  dadao-*-*)
+    noconfigdirs="$noconfigdirs gdb"
+    ;;
   fr30-*-elf*)
     noconfigdirs="$noconfigdirs gdb"
     ;;
-- 
