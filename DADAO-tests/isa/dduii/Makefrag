	and \
	orr \
	xor \
	setzw \
	setow \
	andnw \
	orw \
	setw \
	addi \
	add \
	adrp \
	andi \
	divs \
	divu \
	muls \
	mulu \
	sub \
	cmpu \
	cmps \
	cmp \

